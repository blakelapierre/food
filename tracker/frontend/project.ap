food {
  tracker {
    < {
      ngRoute   :angular-route
    }

    # {
      main {
        search-box {
          search-input {
            search-results
          }
          frequent-results
          inventory-view
          category-view
        }
        tracker-box {
          recent-food
          nutrient-display {
            nutrient-item
          }
        }
      }
    }

    ^ {
      data
    }

    | {
      timeAgo
    }

    > {
      '/': main
    }

    config
  }
}